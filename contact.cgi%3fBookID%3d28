bluedot1_on.src = "images/bluedot_on.gif";
bluedot1_off = new Image(12,12);
bluedot1_off.src = "images/bluedot.gif";

bluedot2_on = new Image(12,12);
bluedot2_on.src = "images/bluedot_on.gif";
bluedot2_off = new Image(12,12);
