cdsign_on.src = "images/cdsign_on.gif";
cdsign_off = new Image(60,20);
cdsign_off.src = "images/cdsign.gif";

}
function imgOn(imgName) {
  if (document.images) {
    document[imgName].src = eval(imgName + "_on.src");
  }
}

function imgOff(imgName) {
  if (document.images)  {
    document[imgName].src = eval(imgName + "_off.src");
  }
}

</script>
