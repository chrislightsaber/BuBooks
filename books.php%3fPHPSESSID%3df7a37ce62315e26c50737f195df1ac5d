	<script type="text/javascript">
function submit_form(frm) {
  
  frm.submit();
  
}

function getElmById(aID){ 
  return (document.getElementById) ? document.getElementById(aID)
                                  : document.all[aID];
} 


function validate_search() {
  var myDropDown = getElmById("dept");
  var myTextfld = getElmById("pattern");
  var errors = 0;
  
  if(myTextfld.value.length < 4) {
    errors++;
    myTextfld.style.background = "red";
    alert("Please enter search terms at least 4 letters long\n or select a department.");
    
    myTextfld.focus();
    myTextfld.style.background = "white";
  }
  
  if(errors>0) {
    return false;
  }
}

function onChangeHandler(e) {
  var myevent = (e) ? e : window.event;
  var pattern = getElmById("pattern");
  pattern.value = "";
  
  submit_form(getElmById("search"));
}

function onChangeHandler2(e) {
  var myevent = (e) ? e : window.event;
  var dept = getElmById("dept");
  dept.selectedIndex = 0;
  
  
  submit_form(getElmById("search"));
}

var myDropDown = getElmById("dept");
if(typeof(myDropDown) == "object") {
  myDropDown.onchange = onChangeHandler;
}

var myTextfld = getElmById("pattern");
if(typeof(myTextfld) == "object") {
  myTextfld.onchange = onChangeHandler2;
}

var mySearchForm = getElmById("search");
mySearchForm.onsubmit = validate_search;

</script>
