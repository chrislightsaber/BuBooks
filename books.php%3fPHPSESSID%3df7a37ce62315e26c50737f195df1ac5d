		function checkform()
		{
			var result;
			result = validate_isbn(document.post.isbn.value);
			if (!result) 
			{
				alert("ISBN " + document.post.isbn.value + " is not a valid textbook. \n Please suggest a different ISBN or manually enter textbook information.\n\n"+" For your information, correct ISBN are 10 characters long and consist of digits 0 through 9 \n with the exception where the tenth character is sometimes the letter 'X' instead of a digit. \n\n For example: \n 0155061399 is correct \n 04714447 is incorrect because it has too few characters \n X748372948 is incorrect because of the letter X in the 1st position \n 189114555X is correct ");
			}
			
			return  result;
		}
		
		
		function validate_isbn(strIsbn)
		{
			var i = 0;
			var test = 0;
			var c;
			strIsbn = new String(strIsbn);

			for (var pos = 0; pos < strIsbn.length; pos++)
			{
				c = strIsbn.substr(pos, 1);
				if (!isNaN(parseInt(c)))
					c -= '0';
				else if (i == 9 && ('X' == c || 'x' == c))
					c = 10;
				else continue;
				test += c * ++i;
				
			}
			return (i == 10 && test % 11 == 0);
		}
		


		//-->

		

