<script type="text/javascript">
function submit_form(frm) 
{  
  frm.submit();  
}

function getElmById(aID)
{ 
  return (document.getElementById) ? document.getElementById(aID) : document.all[aID];
} 

function validate_search() 
{  
  return true;
}

function onChangeHandler(e) 
{
  var myevent = (e) ? e : window.event;
  // Get value of selected option in dept list
  var pattern = getElmById("pattern");
  var selIndex = pattern.selectedIndex;
  // Set value for passing in get
  var index = getElmById("index");
  index.value = selIndex;
  
  submit_form(getElmById("refresh"));
}

function onChangeHandler2(e) 
{
  var myevent = (e) ? e : window.event;

  if (myDropDown2.value != "select" && myDropDown2.value != "oldprofs")
  	submit_form(getElmById("search"));
}

var myDropDown = getElmById("pattern");
if(typeof(myDropDown) == "object") 
{
  myDropDown.onchange = onChangeHandler;
}

var myDropDown2 = getElmById("profid");
if(typeof(myDropDown2) == "object") 
{
	myDropDown2.onchange = onChangeHandler2;
}

var mySearchForm2 = getElmById("search");
mySearchForm2.onsubmit = validate_search2;

var mySearchForm = getElmById("refresh");
mySearchForm.onsubmit = validate_search;
</script>    

