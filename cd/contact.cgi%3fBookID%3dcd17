booksign_on.src = "../images/booksign_ong.gif";
booksign_off = new Image(60,20);
booksign_off.src = "../images/booksigng.gif";

}
function imgOn(imgName) {
  if (document.images) {
    document[imgName].src = eval(imgName + "_on.src");
  }
}

function imgOff(imgName) {
  if (document.images)  {
    document[imgName].src = eval(imgName + "_off.src");
  }
}

